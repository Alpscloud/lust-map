			.st8,
			.st6
				stroke none !important
				

					
	

			
.data-section .map-col .internal-map-contain
	overflow visible

.select2-search--dropdown
	display none


.select2-results__options
	padding 0
	margin 0
	
	li
		list-style-type none
		padding 10px 20px !important
		font-size 16px
		font-family $FuturaHeavy
		color #fff
		background $alternate-color
		margin 0 !important
	



.select2-container--default .select2-results__option--highlighted[aria-selected]
	background-color $basic-black-color
	


.dd-contain
	position relative
	display inline-block
	> span:first-child
		display block
		padding-left 5px
		font-size 13px
		font-weight bold
		color #9A9A9A

	.select2-container
		width auto !important

	.select2-container--default .select2-selection--single,
	.select2-container .select2-selection--single
		background-color transparent
		border 0
		border-radius 0


	.select2-selection__arrow
		display none
	

	.select2-container--default .select2-selection--single .select2-selection__rendered,
	.select2-selection__rendered
		display block
		padding-top 4px
		font-weight normal
		padding 0
		font-size 28px
		line-height 1.4
		font-family $Futura
		text-transform none
		color $basic-black-color
	

	.select2-selection
		display block
		height 100% !important
	

	.selection
		position relative
		display block
		height 40px
		margin-top 1px
		background transparent
		border-top 0
		border-left 0
		border-right 0
		min-width 160px
		

		&:after
			content ''
			position relative
			display block
			width 100%
			height 1px
			background transparent linear-gradient(90deg, #F84481 0%, #FCAE77 100%) 0% 0% no-repeat padding-box
		

		.select2-selection__rendered
			padding-right 24px !important
			padding-left 4px !important

			&:after
				content ''
				position absolute
				display block
				height 36px
				right 0
				bottom 13px
				width 12px
				height 8px
				background url('../img/arw-down.svg') center center no-repeat

		&:focus 
			outline none
			border 0
			
.data-section
	.data-col
		.invert-trigger
			cursor pointer
			path
				transition stroke .17s ease-in-out
			&:hover
				path
					stroke $basic-black-color
		

.data-section
	.expand-control
		display flex
		flex-direction row
		justify-content center
		align-items center
		align-content center
		flex-wrap nowrap
		padding 25px 0
		
		&:before,
		&:after
			content ''
			flex-shrink 1
			position relative
			display block
			width 145px
			height 1px
			background-color #d3d3d3

			

		.expand-toggle
			display flex
			flex-direction row
			justify-content center
			align-items center
			align-content center
			font-family $FuturaHeavy
			flex-wrap nowrap
			flex-shrink 0
			width 127px
			height 38px
			padding 10px 15px
			margin 0 15px
			font-size 12px
			text-transform uppercase
			color $basic-white-color
			background-color $basic-black-color
		

			&:hover
				color $basic-white-color
				background-color $accent-color
				cursor pointer
			

			img
				display block
				margin-right 15px
				flex none
				width 18px

			span
				display block
				text-align center
				flex-grow 1
				

	.table-container
		position relative
		height 304px
		overflow-y hidden
		overflow-x auto
		transition all 0.5s ease
		&.expand,
		&.overlay-active
			height 2059px
			
		&.country-overlay-open .bg-fade
			display none
		
			
		.bg-fade
			position absolute
			top 0
			right 0
			display block
			width 146px
			height 100%
			background transparent linear-gradient(to right, rgba(255,255,255,0), rgba(255,255,255,1)) 0% 0% no-repeat padding-box
			z-index 10
			pointer-events none
			transition: opacity 0.3s

			&.hidden
				opacity 0
				
	.table-contain
		min-width 100%
		opacity 0
		position absolute
		top 100%
		left 0
		overflow-x auto
		overflow-y hidden
		transition all 0.5s linear
		
		h2
			font 18px adobe-caslon-pro,serif
			color #414042
			font-weight 400
			line-height 1.2
			margin-bottom 0.5rem

		&.active
			opacity 1
			top 0
			
		table
			width 100%
			min-width 1000px


			td
				padding 9px 10px 12px
				vertical-align middle
				text-align center
				font-family inherit
				background-color white
				&:nth-child(1)
					width 40px
				
				&:nth-child(2)
					white-space nowrap
					text-align left
					min-width 150px
					width 150px
				
				&:nth-child(6),
				&:nth-child(8)
					min-width 100px
					
				&:nth-child(2)
					min-width 100px
				&:nth-child(13),
				&:nth-child(14)
					min-width 120px
				

			tr
				&:first-child
					font-size 12px
					line-height 1.2
					font-family $FuturaHeavy
					color #9a9a9a
					text-align center
					&:hover
						color #9a9a9a
						background $basic-white-color
				&:nth-child(1n+2)
					cursor pointer
					font 14px $FuturaHeavy
				// &:nth-child(even)
