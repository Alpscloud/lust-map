.map-scale
	position absolute
	top 0
	bottom 0
	left 0
	right 0
	// left -30px
	// right -30px
	background-color white
	z-index 2
	overflow hidden
	transition all .3s ease-in-out
	opacity 0
	visibility hidden
	pointer-events none
	&.active
		opacity 1
		visibility visible
		pointer-events auto
	
	&-container
		// width 100%
		// height 100%
		overflow scroll
		height calc(100% + 25px)
		width calc(100% + 25px)
			
		&.is-hidden
			overflow hidden
	
		
		
		svg
			display block
			width 1800px !important	
			max-width none !important
			margin-top -500px !important
			
			text
