html
	&.is-fixed
		overflow hidden

.data-section
	margin-bottom 80px
	

	.jump-to
		display inline-block
		position relative
		color inherit
		cursor pointer
		span
			position relative
			display inline-block
			z-index 2
		&:before
			content ''
			position absolute
			display block
			bottom 3px
			left -5px
			width calc(100% + 10px)
			height 10px
			background $light-accent-color
			transition all .3s
			z-index 1
		&:hover
			&:before
				height 20px
				
	.map-col
		.control-row
			position relative
			display flex
			justify-content space-between
			flex-wrap nowrap
			padding-left 39px
			z-index 3
			margin-bottom 30px
			
		.languages
			display flex
			justify-content flex-start
			margin-right 25px
			flex-wrap nowrap
			
			img
				display block
				width 26px
				height auto
				margin-right 10px
			
			.language-link
				position relative
				display flex
				justify-content flex-start
				align-items flex-start
				font 13px $FuturaHeavy
				color $basic-black-color
				&:first-child
					margin-right 15px
					
			span.language-link
				span
					position relative
					display inline-block
					&:after
						pseudo()
						display block
						position absolute
						top 100%
						left 0
						width 100%
						height 2px
						background-color currentColor
					
			a.language-link
				color #9a9a9a
				
	.info-trigger
		flex-col-center()
		flex none
		width 39px
		height 39px
		color $basic-white-color
		font 12px $FuturaHeavy
		background-color $basic-black-color
		&:hover
			cursor pointer
			background-color $accent-color
			color $basic-black-color
					
				
	.control-row
		.links-contain
			align-items center
			display flex
			justify-content space-between
			flex-wrap nowrap
			

			
	img.logo
		display block
		width 100%
		
	.logo-wrapper
		width 617px
		position relative
		horisontal-center()
		pointer-events none
		margin-bottom -5%
		z-index -1
		margin-left -32px
		&:before
			pseudo()
			position absolute
			top 0
			right -10px
			display block
			width 30px
			height 100%
			background white
			z-index 1
			pointer-events none
			
		
	
.info-section
	text-align center
	margin-bottom 50px
	
	.site-width
		position relative
		max-width 745px
		width 100%
		text-align left
		margin 0 auto
		

		
