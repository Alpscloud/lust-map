.btn-back
	display inline-block
	width 26px
	height 24px
	fill #333333
	position absolute
	vertical-center()
	left 25px
	

.btn-zoom
	display inline-block
	width 39px
	height 39px
	background-color $basic-black-color
	position absolute
	top 0
	right 0
	z-index 4
	&:hover
		background-color $accent-color
		svg
			fill $basic-white-color
			
	&.is-active
		.btn-zoom__ic-plus
			opacity 0
		.btn-zoom__ic-minus
			opacity 1
	
	&__ic
		svg()
		position absolute
		position-center()
		width 20px
		height 20px
		fill $basic-white-color
		&-minus
			opacity 0
			
